{
    "name": "webignition/simplytestable-base-migrations-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for base Doctrine migrations common to app.simplytestable.com and worker.simplytestable.com",
    "keywords": ["SimplyTestable Migrations"],
    "homepage": "https://github.com/webignition/SimplyTestableBaseMigrationsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",        
        "symfony/symfony": "2.2.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/migrations": "*", 
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/doctrine-migrations-bundle": "*",
        "webignition/container-aware-migration": "*"
    },
    "autoload": {
        "psr-0": { "SimplyTestable\\BaseMigrationsBundle": "" }
    },
    "target-dir": "SimplyTestable/BaseMigrationsBundle"
}
